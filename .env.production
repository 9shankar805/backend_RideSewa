# Production Environment Variables for Render
# Copy these to your Render dashboard environment variables

# Database (Auto-configured by Render)
# DATABASE_URL=postgresql://user:password@host:port/database
# REDIS_URL=redis://host:port

# Server Configuration
NODE_ENV=production
PORT=3001

# Security Configuration
ENCRYPTION_KEY=generate_32_byte_key_here
SESSION_SECRET=generate_session_secret_here
CSRF_SECRET=generate_csrf_secret_here
JWT_SECRET=generate_jwt_secret_here

# Google OAuth (Get from Google Console)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Google Maps API (Your API Key)
GOOGLE_MAPS_API_KEY=AIzaSyCA3uTGHA-w-9nyfne5v1YiiAVRHJU03RE

# Stripe Payment (Get from Stripe Dashboard)
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Twilio SMS (Get from Twilio Console)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Email Configuration (Gmail)
EMAIL_USER=your_gmail_address
EMAIL_PASSWORD=your_gmail_app_password

# AWS S3 (Get from AWS Console)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your_s3_bucket_name

# Firebase (Optional - for enhanced notifications)
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=your_firebase_client_email

# Logging
LOG_LEVEL=info